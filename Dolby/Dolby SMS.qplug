if(not Controls) then return; end;

json = require('rapidjson');

function status(color, str)
  Controls['Connection'].Color = color;
  Controls['Connection'].String = str;
end;

function SoapyHTTP(ep, xml, cb)
  if(Controls.IP.String == '') then return; end;
  HttpClient.Upload({
    Method = 'POST',
    Url = 'http://' .. Controls.IP.String .. '/dc/dcp/json/v1/' .. ep,
    Timeout = 2,
    Data = ([[<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:v1="http://www.doremilabs.com/dc/dcp/json/v1_0"><soapenv:Header/><soapenv:Body>%s</soapenv:Body></soapenv:Envelope>]]):format(xml),
    EventHandler = function(t,c,d,e,h)
      if(Properties['Verbose Debug'].Value == 'Yes') then
        print('< ' .. t.Data);
        if(c ~= 0) then
          print('> ' .. c .. ' ' .. json.encode(d));
        else
          print('ERROR: ' .. e);
        end;
      end;
      if(e and e:match('timed out')) then status('red', 'Disconnected'); return; end;
      if(c == 500 and d:match('not authenticated')) then SESSION = nil; return; end;
      if(c ~= 200) then status('red', 'HTTP Error'); print(t.Url, c,d,e); error('HTTP Error'); return; end;
      local ok, data = pcall(json.decode, d);
      if(ok and cb) then status('green', 'Connected'); cb(data); end;
    end;
  });
end;

function reauth()
  SESSION = 'inprog';
  Timer.CallAfter(function() if(SESSION == 'inprog') then SESSION = nil; end; end, 5);
  if(Controls.Username.String == '') then return; end;
  if(Controls.Password.String == '') then return; end;
  SoapyHTTP('SessionManagement', '<v1:Login><username>admin</username><password>1234</password></v1:Login>', function(r)
    SESSION = r.LoginResponse.sessionId;
    print('# Session acquired: ' .. SESSION)
  end);
end; reauth();
Controls.IP.EventHandler = reauth;
Controls.Username.EventHandler = reauth;
Controls.Password.EventHandler = reauth; 

string.foundIn = function(s,t)
  for _,v in pairs(t) do
    if(v == s) then return true; end;
  end;
  return false;
end;

-- Macro selection
function clearMacros() Macros = {}; end;
function refreshMacros()
  table.sort(Macros);
  for _,c in ipairs(Controls['Macro']) do
    c.Choices = Macros;
    if(c.String == '') then c.String = Macros[1]; end;
    c.Color = c.String:foundIn(c.Choices) and '' or 'orange';
  end;
end;
function addMacro(title, uuid)
  table.insert(Macros, title); --('%s'):format(title, uuid:match('uuid:(.*)')));
  refreshMacros();
end;

-- Macro execution
for i,c in ipairs(Controls['Macro']) do
  c.EventHandler = refreshMacros;
end;
for i,c in ipairs(Controls['Macro Execute']) do
  c.EventHandler = function()
    local name = Controls['Macro'][i].String; --:match('%(([0-9a-f%-]+)%)$');
    SoapyHTTP('MacroManagement', ('<v1:RunMacro><sessionId>%s</sessionId><name>%s</name></v1:RunMacro>'):format(SESSION, name));
  end;
end;

PollTimer = Timer.New();
PollTimer:Start(1);
PollTimer.EventHandler = function()
  
  if(not SESSION) then reauth(); end;
  if(SESSION == 'inprog') then return; end;
  
  -- Poll Current Status
  SoapyHTTP('SystemOverview', ('<v1:GetSystemOverview><sessionId>%s</sessionId></v1:GetSystemOverview>'):format(SESSION), function(r)
    Controls['Current CPL'].String = r.GetSystemOverviewResponse.playback.cplTitle;
    Controls['Current SPL'].String = r.GetSystemOverviewResponse.playback.splTitle;
    Controls['SPL Position'].Position = r.GetSystemOverviewResponse.playback.splPosition / r.GetSystemOverviewResponse.playback.splDuration;
    Controls['SPL Status'].String = r.GetSystemOverviewResponse.playback.stateInfo;
  end);
  
  -- Poll available macros
  if(not Macros or os.time() % 360 == 0) then
    print('# Refreshing macro list...');
    SoapyHTTP('MacroManagement', ('<v1:GetMacroList><sessionId>%s</sessionId></v1:GetMacroList>'):format(SESSION), function(r)  
      clearMacros();
      for _,uuid in ipairs(r.GetMacroListResponse.macroList.uuid) do
        SoapyHTTP('MacroManagement', ('<v1:GetMacroInfo><sessionId>%s</sessionId><uuid>%s</uuid></v1:GetMacroInfo>'):format(SESSION, uuid), function(r)
          addMacro(r.GetMacroInfoResponse.macroInfo.title, r.GetMacroInfoResponse.macroInfo.uuid);
        end);
      end;
    end);
  end;
  
  -- Monitor Ingest Status
  SoapyHTTP('IngestManagement', ('<v1:GetIngestTaskList><sessionId>%s</sessionId></v1:GetIngestTaskList>'):format(SESSION), function(r)
    local ingest = false;
    for _,t in ipairs(r.GetIngestTaskListResponse.taskList.ingestTask) do
      if(t.state == 'Running') then ingest = t.taskId; end;
    end;
    Controls['Ingest Status'].Boolean = ingest;
    Controls['Ingest Status'].Color = ingest and 'lime' or '';
    Controls['Ingest Status'].Legend = ingest and 'INGESTING' or 'IDLE';
    if(ingest) then
      SoapyHTTP('IngestManagement', ('<v1:GetIngestTaskStatus><sessionId>%s</sessionId><taskId>%s</taskId></v1:GetIngestTaskStatus>'):format(SESSION, ingest), function(r)
        Controls['Ingest Progress'].Position = tonumber(r.GetIngestTaskStatusResponse.taskStatus.totalProgress) / 100;
      end);
    else
      Controls['Ingest Progress'].Value = 0;
    end;
  end);
  
end;

ShutdownTimer = Timer.New();
ShutdownTimer.EventHandler = function()
  if(ShuttingDown == nil) then ShuttingDown = 10;
  else ShuttingDown = ShuttingDown - 1; end;
  
  if(ShuttingDown > 0) then
    Controls['Shutdown Countdown'].String = ('Shutting down in %d...'):format(ShuttingDown);
  else
    ShutdownTimer:Stop(); ShuttingDown = nil;
    Controls['Shutdown Countdown'].String = 'Shutting down...';
    print('# Sending shutdown command...');
    SoapyHTTP('PowerManagement', ('<v1:Shutdown><sessionId>%s</sessionId><delayMinutes>0</delayMinutes></v1:Shutdown>'):format(SESSION), function()
      Controls['Shutdown Countdown'].String = '';
    end);
  end;
end;

Controls['Shutdown'].EventHandler = function() ShutdownTimer:Start(1); end;
Controls['Cancel Shutdown'].EventHandler = function()
  ShutdownTimer:Stop(); ShuttingDown = nil;
  Controls['Shutdown Countdown'].String = 'Shutdown cancelled';
end;

RebootTimer = Timer.New();
RebootTimer.EventHandler = function()
  if(Rebooting == nil) then Rebooting = 10;
  else Rebooting = Rebooting - 1; end;
  
  if(Rebooting > 0) then
    Controls['Reboot Countdown'].String = ('Rebooting in %d...'):format(Rebooting);
  else
    RebootTimer:Stop(); Rebooting = nil;
    Controls['Reboot Countdown'].String = 'Rebooting...';
    print('# Sending Reboot command...');
    SoapyHTTP('PowerManagement', ('<v1:Reboot><sessionId>%s</sessionId><delayMinutes>0</delayMinutes></v1:Reboot>'):format(SESSION), function()
      Controls['Reboot Countdown'].String = '';
    end);
  end;
end;

Controls['Reboot'].EventHandler = function() RebootTimer:Start(1); end;
Controls['Cancel Reboot'].EventHandler = function()
  RebootTimer:Stop(); Rebooting = nil;
  Controls['Reboot Countdown'].String = 'Reboot cancelled';
end;
